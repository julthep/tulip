                    rdfs:label "Current M4A1 carbine with detachable carrying handle." ;
                    tlp:link
                        [
                            tlp:image <http://upload.wikimedia.org/wikipedia/commons/thumb/a/a1/Coltm4a1.jpeg/120px-Coltm4a1.jpeg> ;
